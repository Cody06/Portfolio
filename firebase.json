{
    "hosting": {
        "public": "out",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [
            {
                "source": "/workboard",
                "destination": "/workboard.html"
            },
            {
                "source": "/social",
                "destination": "/social.html"
            }
        ]
    },
    "emulators": {
        "hosting": {
            "port": 5033
        },
        "ui": {
            "enabled": true
        },
        "singleProjectMode": true
    }
}
